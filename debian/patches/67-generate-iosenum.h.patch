+			$(CINTTMP) $(CINTTMPINC) \
+			        -I$(ROOT_SRCDIR)/cmake/scripts -Iinclude \
+				$(IOSENUMC) > /dev/null; \
 			mv iosenum.h $@; \
 		else \
 			touch $@; \
